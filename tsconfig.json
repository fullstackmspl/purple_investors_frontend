{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "skipLibCheck": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["node", "googlemaps"],
    "lib": [
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false
  }
}
